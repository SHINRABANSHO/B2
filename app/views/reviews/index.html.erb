<p id="notice"><%= notice %></p>

<h1>Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Service_id</th>
      <th>User_id</th>
      <th>Valuation</th>
      <th>Promotion score</th>
      <th>Price</th>
      <th>Starting cost</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td><%= review.name %></td>
        <td><%= review.service_id %></td>
        <td><%= review.user_id %></td>
        <td><%= review.valuation %></td>
        <td><%= review.promotion_score %></td>
        <td><%= review.price %></td>
        <td><%= review.starting_cost %></td>
        <td><%= review.content %></td>
        <td><%= link_to 'Show', review %></td>
        <td><%= link_to 'Edit', edit_review_path(review) %></td>
        <td><%= link_to 'Destroy', review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review', new_review_path %>
